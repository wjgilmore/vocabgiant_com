  },
  "homepage": "https://github.com/wjgilmore/vocabgiant",
  "dependencies": {
    "body-parser": "^1.12.4",
    "bootstrap": "^3.3.5",
    "express": "^4.12.4",
    "express-jsonstream": "0.0.3",
    "lodash": "^3.10.0",
    "mongoose": "^4.0.6",
    "mongoose-url-slugs": "^0.1.3",
    "react": "^0.13.3",
    "react-bootstrap": "^0.23.2",
    "react-dom": "^0.14.0-beta1",
    "react-router": "^0.13.3",
    "react-swipe": "^2.1.0",
    "reflux": "^0.2.8",
    "superagent": "^1.2.0",
    "swipe-js-iso": "^2.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^3.1.17",
    "babel-jest": "^5.3.0",
    "babelify": "^6.1.2",
    "browserify": "^10.2.3",
    "eslint": "^0.24.1",
    "eslint-plugin-react": "^2.7.0",
    "gulp": "^3.9.0",
